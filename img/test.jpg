#cookiescript_injected {
bottom: 0;
    left: 0;
    right: 0;
    position: fixed;
    padding: 15px 20px 10px 20px;
    width: 100%;
    display: flex;
    flex-direction: column;
    text-align: center;
    margin: 0;
    align-items: center;
    max-height: 85%;
    overflow-y: auto;
}
#cookiescript_copyright {
    line-height: 1;
    text-align: center;
}
#cookiescript_buttons {
    justify-content: center;
    flex-wrap: wrap;
    margin: 0 0 10px 0;
}
#cookiescript_manage_wrap {
    margin: 0 0 14px 0;
}
#cookiescript_header {
    padding: 3px 0 8px;
